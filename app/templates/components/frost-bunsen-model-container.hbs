{{#if renderLabel}}
  {{#frost-bunsen-section
    className="model-container"
    collapsible=collapsible
    id=(concat elementId '-s')
    instructions=instructions
    required=isRequired
    title=renderLabel
  }}
    {{frost-bunsen-container
      bunsenId=bunsenId
      cellConfig=cellConfig
      errors=errors
      id=(concat elementId '-c')
      model=model
      onChange=onChange
      readOnly=readOnly
      store=store
      value=value
    }}
  {{/frost-bunsen-section}}
{{else}}
  {{frost-bunsen-container
    bunsenId=bunsenId
    cellConfig=cellConfig
    errors=errors
    id=(concat elementId '-c')
    model=model
    onChange=onChange
    readOnly=readOnly
    store=store
    value=value
  }}
{{/if}}
