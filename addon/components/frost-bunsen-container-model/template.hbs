{{#if renderLabel}}
  {{#frost-bunsen-section
    className="container-model"
    collapsible=collapsible
    instructions=instructions
    required=isRequired
    title=renderLabel
  }}
    {{frost-bunsen-container
      bunsenId=bunsenId
      cellConfig=cellConfig
      model=model
      on-change=on-change
      readOnly=readOnly
      store=store
    }}
  {{/frost-bunsen-section}}
{{else}}
  {{frost-bunsen-container
    bunsenId=bunsenId
    cellConfig=cellConfig
    model=model
    on-change=on-change
    readOnly=readOnly
    store=store
  }}
{{/if}}
