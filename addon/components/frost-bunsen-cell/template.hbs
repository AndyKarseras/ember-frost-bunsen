{{#if config.model}}
  {{#if isSubModelObject}}
    {{frost-bunsen-container-model
      bunsenId=renderId
      cellConfig=config
      model=subModel
      on-change=on-change
      readOnly=readOnly
      required=required
      store=store
    }}
  {{else if isSubModelArray}}
    {{frost-bunsen-container-array
      bunsenId=renderId
      cellConfig=config
      model=subModel
      on-change=on-change
      readOnly=readOnly
      required=required
      store=store
    }}
  {{else}}
    {{frost-bunsen-input
      bunsenId=renderId
      cellConfig=config
      initialValue=initialValue
      model=subModel
      on-change=on-change
      readOnly=readOnly
      required=required
      store=store
    }}
  {{/if}}
{{else if config.container}}
  {{frost-bunsen-container
    cellConfig=config
    model=model
    on-change=on-change
    readOnly=readOnly
    store=store
  }}
{{/if}}
